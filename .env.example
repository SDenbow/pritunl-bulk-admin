# Postgres
POSTGRES_DB=pritunl_admin
POSTGRES_USER=pritunl_admin
POSTGRES_PASSWORD=change_me

DATABASE_URL=postgresql+psycopg://pritunl_admin:change_me@db:5432/pritunl_admin

# Session signing (32+ chars)
SESSION_SECRET=change_me_to_a_long_random_value

# Master key for encrypting target credentials (base64, 32-byte key)
# Generate with:
#   python3 -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
PRITUNL_UI_MASTER_KEY=PASTE_GENERATED_KEY_HERE

# First-boot setup token (required to access /setup before first admin exists)
SETUP_TOKEN=change_me_setup_token

# Dangerous operations
ALLOW_DELETE=false
